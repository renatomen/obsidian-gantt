{
  "name": "obsidian-gantt",
  "version": "0.0.1",
  "private": true,
  "description": "Generate DHTMLX Gantt charts from Obsidian note properties and YAML frontmatter metadata.",
  "author": "Renato / Open Source Community",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "node scripts/build.mjs",
    "postbuild": "node scripts/install-to-vault.cjs",
    "dev": "node scripts/build.mjs --watch",
    "test": "jest --passWithNoTests",
    "lint": "eslint . --ext .ts",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "format": "prettier --write .",
    "e2e": "wdio run ./test/wdio/wdio.conf.mts",
    "build:local": "node scripts/build-local.mjs",
    "e2e:local": "node scripts/e2e-local.mjs",
    "prepare": "husky"
  },
  "devDependencies": {
    "@swc/core": "^1.13.5",
    "@swc/jest": "^0.2.39",
    "@types/jest": "^30.0.0",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "@wdio/cli": "^9.19.2",
    "@wdio/globals": "^9.17.0",
    "@wdio/mocha-framework": "^9.19.2",
    "@wdio/types": "^9.19.2",
    "esbuild": "^0.25.10",
    "eslint": "^9.36.0",
    "husky": "^9.1.7",
    "jest": "^30.1.3",
    "obsidian-typings": "^4.51.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.4",
    "typescript": "^5.9.2",
    "wdio-obsidian-reporter": "^2.1.2",
    "wdio-obsidian-service": "^2.1.2",
    "webdriverio": "^9.19.2"
  }
}
